                    "properties": {
                        "onprem": {
                            "$id": "/properties/resources/properties/volumes/properties/onprem",
                            "type": "array",
                            "items": {
                                "$id": "/properties/resources/properties/volumes/properties/onprem/items",
                                "type": "string",
                                "title": "The 0th Schema ",
                                "default": "",
                                "examples": [
                                    "certificateserver.yaml",
                                    "mysqlserver.yaml",
                                    "rabbitmq-cert.yaml",
                                    "rabbitmq.yaml"
                                ]
                            }
