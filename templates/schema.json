                    "$id": "/properties/resources/properties/configmaps",
                    "type": "array"
                },
                "roles": {
                    "$id": "/properties/resources/properties/roles",
                    "type": "array"
                },
                "volumes": {
                    "$id": "/properties/resources/properties/volumes",
