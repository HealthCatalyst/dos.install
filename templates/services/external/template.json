{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "$name$public",
        "namespace": "$namespace$"
    },
    "spec": {
        "selector": {
            "app": "$name$"
        },
        "ports": [{
            "port": 80,
            "targetPort": "$port$"
        }]
    }
}